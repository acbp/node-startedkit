{
    "app": {
        "port": ${PORT},
        "env": "${ENV}",
        "host": "${HOST}"
    },
    "log": {
        "gelf": {
            "port": ${GELF_PORT},
            "level": "${GELF_LEVEL}",
            "name": "${GELF_NAME}",
            "host": "${GELF_HOST}",
            "enabled": ${GELF_ENABLED},
            "source": ${GELF_SOURCE},
            "facility": "${GELF_FACILITY}"
        },
        "console": {
            "level": "${CONSOLE_LEVEL}"
        }
    }
}
